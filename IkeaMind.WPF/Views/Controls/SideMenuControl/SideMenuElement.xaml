<UserControl x:Class="IkeaMind.WPF.Views.Controls.SideMenuControl.SideMenuElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IkeaMind.WPF.Views.Controls.SideMenuControl" 
             xmlns:controls="clr-namespace:IkeaMind.WPF.ViewModels.Controls" 
             d:DataContext="{d:DesignInstance Type=controls:SideMenuElementViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="40">
    <DockPanel Width="40" Height="40">
        <Image Source="{Binding ImageSource}" />
    </DockPanel>
</UserControl>
